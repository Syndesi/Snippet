{
  "source":{
    "include":[
      "./client/"
    ],
    "includePattern":".+\\.js(doc|x)?$",
    "excludePattern":"(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "linenums":true,
  "opts":{
    "encoding":"utf8",
    "recurse":true,
    "private":false,
    "lenient":true,
    "destination":"./docs",
    "template":"./node_modules/jaguarjs-jsdoc"
  },
  "templates":{
    "applicationName":"Dungeon",
    "disqus":"",
    "googleAnalytics":"",
    "openGraph":{
      "title":"",
      "type":"website",
      "image":"",
      "site_name":"",
      "url":"https://github.com/Syndesi/Snippet"
    },
    "meta":{
      "title":"",
      "description":"",
      "keyword":""
    }
  }
}